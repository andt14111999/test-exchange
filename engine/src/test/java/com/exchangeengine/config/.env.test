# Cấu hình môi trường
APP_ENV=test

# Cấu hình Kafka
KAFKA_BOOTSTRAP_SERVERS=test-server:9092
KAFKA_TOPIC_PARTITIONS=1
KAFKA_TOPIC_REPLICATION_FACTOR=1

# Cấu hình RocksDB
ROCKSDB_DATA_DIR=./data/rocksdb/test
ROCKSDB_MAX_OPEN_FILES=10
ROCKSDB_WRITE_BUFFER_SIZE=4
ROCKSDB_MAX_WRITE_BUFFER_NUMBER=2
ROCKSDB_TARGET_FILE_SIZE_BASE=2

# Cấu hình Disruptor
DISRUPTOR_BUFFER_SIZE=1024
DISRUPTOR_WAIT_STRATEGY=blocking

# Cấu hình ứng dụng
APP_LOG_LEVEL=DEBUG
